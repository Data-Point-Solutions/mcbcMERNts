{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["src"],
      "exclude": ["node_modules", "dist"],
      "includePattern": ".+\\.(js|ts)$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "destination": "./docs/jsdoc",
      "recurse": true,
      "verbose": true
    }
  }
  