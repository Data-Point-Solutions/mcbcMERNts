{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true
    },
    "include": [
      "backend/**/*.ts",
      "frontend/src/**/*.ts",
      "frontend/src/**/*.tsx"
    ]
  }
  